@import "../../template/template.wxss";

page{
  background-color: white;
}
.pro_de_head{
  padding: 0 3%;
  height: 200rpx;
  padding-top: 1rpx;
  color: white;
  background-color: #e50012;
}
.pro_de_head_one{
  font-size: 24rpx;
  margin-top: 24rpx;
}
.pro_de_head_two{
  margin-top: 45rpx;
  font-size: 60rpx;
}
.pro_de_one{
  height: 57rpx;
  background-color: white;
  font-size: 24rpx;
  color: #232323;
  display: flex;
  justify-content: space-around;
}
.pro_de_one>view{
  text-align: center;
  line-height: 56rpx;
}
.pro_de_one_sel{
  color: #e50012;
  border-bottom: 1rpx solid #e50012;
}
.pro_de_two{
  padding-bottom: 20rpx;
}
.p_two_list_head{
  padding: 0 3%;
  height: 155rpx;
  background-color: #fafafa;
}
.list_head_L{
  float: left;
}
.list_head_L>view:nth-child(1){
  color: #232323;
  font-size: 30rpx;
  margin-top: 44rpx; 
}
.list_head_L>view:nth-child(2){
  color: #757575;
  font-size: 24rpx;
  margin-top: 14rpx; 
}
.list_head_R{
  float: right;
  width: 47rpx;
  height: 47rpx;
  margin-top: 56rpx;
}
.p_two_list_cont{
  border: 1px solid white; 
  padding: 0 3%;
  background-color: white;
}
.list_cont_list{
  border-bottom: 1rpx solid #e5e5e5;
  height: 84rpx;
  margin-top: 20rpx;
}
.list_cont_list_L{
  float: left;
  font-size: 24rpx;
}
.list_cont_list_L>view:nth-child(1){
  color: #232323;
}
.list_cont_list_L>view:nth-child(2){
  color: #868686;
}
.list_cont_list_R{
  float: right;
  color: #232323;
  font-size: 30rpx;
  line-height: 84rpx;
}





/*日历icon*/
.calendarIcon{
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
}