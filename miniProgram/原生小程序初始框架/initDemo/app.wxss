view,text,input,label,form,textarea{
  margin: 0;
  padding: 0;
}

page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  font-family: '微软雅黑';
}

.divHide {
  display: none !important;
}

.div_float:after {
  clear: both;
  content: '';
  width: 0;
  height: 0;
  visibility: hidden;
  display: block;
}

.eli_one {
  overflow: hidden;
  text-overflow: ellipsis;
  display: box !important;
  display: -webkit-box !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.eli_two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: box !important;
  display: -webkit-box !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}