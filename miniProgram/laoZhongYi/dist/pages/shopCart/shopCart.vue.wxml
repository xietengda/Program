<template name="639b0986">
  <view class="_div data-v-6f33a7e8 container">
    <view class="_div data-v-6f33a7e8 cartHead div_float">
      <view class="_div data-v-6f33a7e8 L" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}"><label class="_span data-v-6f33a7e8 cb_no {{[allSel ? 'cb_sel' : '']}}"></label>全选 </view>
      <view class="_div data-v-6f33a7e8 R"><label wx:if="{{!editBtn}}" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_span data-v-6f33a7e8">编辑</label><label wx:else bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_span data-v-6f33a7e8">完成</label></view>
    </view>
    <view class="_div data-v-6f33a7e8 cartView">
      <view class="_div data-v-6f33a7e8 proListView">
        <view class="_div data-v-6f33a7e8 proList" wx:key="item.id" key="{{item.id}}" wx:for="{{cartList}}" wx:for-index="key" wx:for-item="item">
          <view class="_div data-v-6f33a7e8 L"><label class="_span data-v-6f33a7e8 cb_no {{[item.sel ? 'cb_sel' : '']}}" bindtap="handleProxy" data-sel-index="{{key}}" data-eventid="{{'3-'+key}}" data-comkey="{{$k}}"></label></view>
          <view class="_div data-v-6f33a7e8 M">
            <image src="{{item.img}}" class="_img data-v-6f33a7e8"></image>
            <view class="_div data-v-6f33a7e8 Msg">
              <view class="_div data-v-6f33a7e8 til">{{item.til}}</view>
              <view class="_div data-v-6f33a7e8 ml">规格：{{item.ml}}</view>
              <view class="_div data-v-6f33a7e8 price">¥{{item.price}}</view>
            </view>
          </view>
          <view class="_div data-v-6f33a7e8 num"><label bindtap="handleProxy" data-c-type="cut" data-c-index="{{key}}" data-eventid="{{'4-'+key}}" data-comkey="{{$k}}" class="_span data-v-6f33a7e8">-</label> <input type="number" value="{{item.num}}" disabled="disabled" class="_input data-v-6f33a7e8" /> <label bindtap="handleProxy" data-c-type="add" data-c-index="{{key}}" data-eventid="{{'5-'+key}}" data-comkey="{{$k}}" class="_span data-v-6f33a7e8">+</label></view>
        </view>
      </view>
    </view>
    <view class="_div data-v-6f33a7e8 cartBtm div_float">
      <view class="_div data-v-6f33a7e8 R" wx:if="{{!editBtn}}">
        <view class="_div data-v-6f33a7e8 rl">
          <view class="_div data-v-6f33a7e8">总计：¥{{countPrice}}</view>
          <view class="_div data-v-6f33a7e8 freight">运费：¥0.00</view>
          <view class="_div data-v-6f33a7e8">全场满128包邮</view>
        </view>
        <view class="_div data-v-6f33a7e8 rr">结算（{{countToatl}}）</view>
      </view>
      <view class="_div data-v-6f33a7e8 delR" wx:else>
        <view class="_div data-v-6f33a7e8 del">删除</view>
      </view>
    </view>
  </view>
</template>