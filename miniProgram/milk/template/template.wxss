/*头部*/
.index_head{
  width: 100%;
  height: 75rpx;
  text-align: center;
  position: fixed;
  padding-top: 50rpx;
  z-index: 6;
  background-color: white;
  top: 0;
}
.headIconView{
  position: absolute;
  display: inline-block;
  color: #353535;
  font-size: 28rpx;
  left: 10rpx;
  top: 50rpx;
  line-height: 75rpx;
}
.backIcon{
  width: 35rpx;
  height: 55rpx;
  float: left;
  margin-top: 10rpx;
}
.head_logo{
  width: 152rpx;
  height: 52rpx;
  display: inline-block;
  margin-top: 10rpx;
  background: url('data:image/jpeg;base64,') center center no-repeat;
  background-size: 100% 100%;
}


/*倒计时*/
.timeLimt_list_time{
  color: #353535;
  font-size: 16rpx;
}
.timeLimt_list_time>text{
  display: inline-block;
  width: 33rpx;
  height: 33rpx;
  color: white;
  font-size: 20rpx;
  font-weight: bold;
  background-color:#353535; 
  margin: 0rpx 5rpx;
}
/*搜索框*/
.t_search_view{
  height: 85rpx;
  background-color: white;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  padding:0 2%;
  position: relative;
  border: 1px solid white;
}
 .t_search_view>input{
  width: 65%;
  height: 50rpx;
  border-radius: 10rpx;
  background-color: #e0e0e0;
  padding-left: 9%;
  margin-top: 15rpx;
  font-size: 20rpx;
  color: #333;
  line-height: 45rpx;
}
.input_bg{
  position: absolute;
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  background-size: 100% 100%;
  top: 25rpx;
  margin-left: 2%;
}
.search_pla{
  color: #989898;
}
.t_search_btn{
  position: absolute;
  top: 15rpx;
  right: 2%;
  width: 20%;
  height: 50rpx;
  color: white;
  font-size: 24rpx;
  line-height: 50rpx;
  background-color: #e50012;
}
/*二级标题*/
.action_div{
  height: 65rpx;
}
.action_div>image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin-top: 12rpx;
  margin-left: 30rpx;
}
.action_div2>image{
  width: 30rpx;
}
.action_div3>image{
  height: 45rpx;
  margin-top: 10rpx;
}
.action_div>text{
  color: #353535;
  font-size: 32rpx;
  line-height: 65rpx;
  margin-left: 10rpx;
}


/*领取优惠券*/
.index_discount{
  width: 98%;
  margin: 0 auto;
  height: 156rpx;
  white-space: nowrap;
}
.index_discount_list{
  width: 240rpx;
  height: 110rpx;
  background: url('data:image/jpeg;base64,') center center no-repeat;
  background-size: 100% 100%;
  margin: 20rpx 0rpx;
  padding-left: 10rpx;
  display: inline-block;
  margin-right: 40rpx;
}

.index_discount_list>text{
  float: left;
  font-size: 24rpx;
  color: #ff3338;
  line-height: 24rpx;
  margin-left: 10rpx;
  margin-top: 20rpx;
}
.discount_list_L{
  float: left;
  margin-top: 10rpx;
}
.discount_list_L>text{
   font-size: 62rpx; 
  color: #f62736;
}
.discount_list_R{
  float: right;
  width: 48%;
  height: 100%;
  display: inherit;
}
.discount_list_R>text:nth-child(1){
  font-size: 16rpx;
  color: #f92d3d;
  display: inherit;
  margin-top: 25rpx;
}
.discount_list_R>text:nth-child(2){
  font-size: 12rpx;
  color: #f92d3d;
  display: inherit;
  width: 60rpx;
  padding-right: 15rpx;
  height: 18rpx;
  text-align: center;
  line-height: 20rpx;
  border-radius: 50rpx;
  margin-top: 15rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAAFCAYAAABirU3bAAAAQUlEQVQImU3MMQ2AMBRAwWvzR5yQtFLwQUIanCAFzGAAC+wwwNA33vDSU+qOBRdkTDjREOFrwIYx/3BjxRw4+scLxXUJmUMjH0gAAAAASUVORK5CYII=')   60rpx 5rpx  no-repeat;
  background-color: white;
}

/*黑色遮罩层*/
.black_layer{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 4;
  background-color: rgba(0,0,0,0.6)
}

/*商品展示*/
.resh_view{
  width: 100%;
  height: 375rpx;
  background-color: white;
  margin-bottom: 10rpx;
}
.resh_view_L{
  float: left;
  width: 35%;
  height: 100%;
  font-size: 0;
  -webkit-text-size-adjust: none;
  text-align: center;
}
.resh_view_time{
  display: inline-block;
  margin-top: 80rpx;
}
.resh_view_L>navigator>image{
  width: 220rpx;
  height: 135rpx;
  margin-top: 80rpx;
}
.resh_view_R{
  float: left;
  width: 60%;
  height: 100%;
  margin-left: 5%;
}
.resh_til{
  color: #353535;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 85rpx;
}
.resh_subTil{
  color: #232323;
  font-size: 18rpx;
  margin-top: 20rpx;
}
.resh_subView{
  position: relative;
  margin-top: 90rpx;
}
.resh_subView_L{
  float: left;
}
.resh_oldPrice{
  color: #3e403f;
  font-size: 16rpx;
  text-decoration: line-through;
}
.resh_oldPrice>text{
  color: #E50012;
  font-size: 26rpx;
  font-weight: bold;
}
.resh_newPrice{
  font-size: 24rpx;
  color: #3e403f;
}
.resh_newPrice>text{
  font-size: 42rpx;
  color: #E50012;
  font-weight: bold;
}
.resh_subView_R{
  position: absolute;
  margin:auto;
  top: 0;
  bottom: 0;
  right: 20rpx;
  width: 230rpx;
  height: 52rpx;
  border-radius: 20rpx;
  background-color: #E50012; 
  font-size: 24rpx;
  color: white;
  line-height: 52rpx;
}
 .subView_clo{
  width: 33rpx;
  height: 33rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
  float: left;
} 
.resh_subView_R>text:nth-child(2){
  float: left;
  margin-left: 20rpx;
  padding-left: 20rpx;
  border-left: 1rpx solid white;
}
/*其他商品展示*/
.resh_view_other{
  height: 265rpx;
  margin-bottom: 0rpx;
}
.resh_til_other{
  margin-top: 25rpx;
}
.resh_subView_other{
  margin-top: 50rpx;
}
/*上传图片*/
.ev_photo_list{
  float: left;
  width: 150rpx;
  height: 150rpx;
  position: relative;
  border-radius: 20rpx;
  margin-right: 10rpx;
  margin-bottom: 20rpx;
}
.ev_photo_list>image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.ev_photo_list>text{
  border-radius: 100%;
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  background-color: #e50012;
  font-size: 50rpx;
  text-align: center;
  line-height: 35rpx;
  color: white;
  display: inline-block;
  top: -6rpx;
  right: -1rpx;
  border: 5rpx solid white;
}
.upLoad_icon{
  float: left;
  width: 155rpx;
  height: 150rpx;
  position: relative;
  border-radius: 20rpx;
  border: 1rpx solid #ccc;
  text-align:center;
  margin-bottom: 20rpx;
}
.upLoad_icon>image{
  width: 68rpx;
  height: 57rpx;
  margin-top: 30rpx;
}
.upLoad_icon>view{
  font-size: 22rpx;
  color: #353535;
  margin-top: 15rpx;
}
.o_evla_sub{
  width: 100%;
  height: 78rpx;
  background-color: #e50012;
  border-radius: 20rpx;
  color: white;
  font-size: 28rpx;
  margin-top: 80rpx;
}

/*图片广告*/
.sco_cont{
  margin-top: 10rpx;
  background-color: white;
  text-align: center;
  padding-top: 10rpx;
}
.sco_cont>image{
  width: 95%;
  height: 205rpx;
  margin-bottom: 10rpx;
}

/*优惠券*/
.discount_vew{
  padding-top: 15rpx;
}
.dis_cont_list{
  width: 95%;
  height:142rpx;
  margin: 0 auto; 
  margin-bottom: 30rpx;
  background-size: 100% 100% !important;
  position: relative;
  padding-top: 1rpx;
  margin-top: 15rpx;
}
.dis_list_L{
  border: 1rpx solid white;
  border-radius: 10rpx;
  width: 73%;
  height: 90%;
  margin-left: 35rpx;
  margin-top: 5rpx;
}
.dis_list_L_one{
  float: left;
  color: #e50012;
  font-size: 92rpx;
  font-weight: bold;
  margin-left: 10rpx;
}
.dis_list_L_one>text{
  font-size: 28rpx;
}
.dis_list_L_two{
  float: left; 
  margin-left: 30rpx;
}
.dis_list_L_two>view:nth-child(1){
  color: #232323;
  font-size: 24rpx;
  margin-top: 30rpx;
}
.dis_list_L_two>view:nth-child(2){
  color: #707070;
  font-size: 20rpx;
  margin-top: 15rpx;
}
.dis_list_R{
  position: absolute;
  right: 0;
  top: 0;
  width: 17.5%;
  height: 100%;
}
.dis_list_R>view{
  height: 100%;
  color: white;
  font-size: 20rpx;
  text-align: center;
  background-size: 100% 100%;
}
.dis_list_R>view>view:nth-child(1){
  padding-top: 40rpx;
}
.dis_btn{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  background-color: #e50012;
  color: white;
  letter-spacing: 5rpx;
  font-size: 30rpx;
  border-radius: 0;
  line-height: 90rpx;
}
.dis_btn>text{
  color: #f19699;
}

