@import '../../template/template.wxss';

.add_cont{
  margin-top: 20rpx;
  padding-bottom: 130rpx;
}
.cont_list{
  padding: 0 5%;
  background-color: white;
  margin-bottom: 20rpx;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}
.cont_list_one{
  line-height: 60rpx;
}
.cont_list_one_L{
  font-size: 28rpx;
  color: #232323;
  float: left;
}
.cont_list_one>navigator{
  display: block;
}
.cont_list_one_L>text{
  font-size: 24rpx;
  margin-left: 20rpx;
}
.cont_list_one_R{
  float: right;
  color: #535353;
  font-size: 24rpx;
}
.cont_list_one_R>text{
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  vertical-align: sub;
  margin-right: 15rpx;
}
.cont_list_two{
  line-height: 100rpx;
  color: #555555;
  font-size: 24rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.cont_list_third{
  line-height: 60rpx;
}
.cont_list_third_L{
  float: left;
  font-size: 24rpx;
  color: #232323;
}
.cont_list_third_L>text{
  display: inline-block;
  width: 18rpx;
  height: 18rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 100%;
  margin-right: 15rpx;
}
.setDefault{
  border-color: #e50012;
  background-color: #e50012;
}
.cont_list_third_R{
  float: right;
  color: #535353;
  font-size: 24rpx;
}
.cont_list_third_R>text{
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  vertical-align: sub;
  margin-right: 15rpx;
}
.add_addr_btm{
  position: fixed;
  width: 100%;
  height: 78rpx;
  bottom: 0;
  text-align: center;
  background-color: white;
  padding-top: 10rpx;
  padding-bottom: 25rpx;
}
.add_addr_btm>navigator{
  display: block;
}
.add_addrBtn{
  width: 90%;
  border-radius: 30rpx;
  line-height: 78rpx;
  font-size: 28rpx;
  color: white;
  background-color: #e50012;
}