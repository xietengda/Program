@import "../../template/template.wxss";

.payFin_head{
  height:250rpx;
  background-color: white; 
  text-align: center;
  font-size: 24rpx;
  color: #808080;
}
.payFin_head>icon{
  margin-top: 50rpx;
}
.payFin_head>view:nth-child(2){
  margin-top:15rpx; 
}
.payFin_head>view:nth-child(3){
  margin-top:15rpx; 
}
.payFin_head_btn{
  margin-top: 10rpx;
  background-color: white;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-around;
  font-size: 24rpx;
  align-items: center;
}
.payFin_head_btn view{
  display: inline-block;
  border: 1rpx solid #808080;
  padding: 0 30rpx;
  height: 50rpx;
  border-radius: 20rpx;
  line-height: 50rpx;
  color: #808080;
}
.payFin_head_btn>navigator:nth-child(2)>view{
  background-color: #e50012;
  border: 1rpx solid #e50012;
  color: white;
}
.payFin_QRcode{
  width: 95%;
  height: 354rpx;
  margin: 0 auto;
  background-color: white;
  margin-top: 10rpx;
  text-align: center;
  border-top: 1rpx solid white;
}
.payFin_QRcode>view>text{
  display: inline-block;
  width: 24rpx;
  height: 35rpx;
  vertical-align: sub;
  margin-right: 10rpx;
}
.payFin_QRcode_til{
  color: #6e6e6e;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.payFin_QRcode>image{
  width: 190rpx;
  height: 190rpx;
  margin: 0 auto;
  margin-top: 15rpx;
}
.payFin_QRcode_num{
  margin-top:20rpx;
  color: #000;
  font-size: 36rpx;
  letter-spacing: 5rpx;
}
.payFin_btm{
  width: 95%;
  height: 220rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.payFin_btm>image{
  width: 100%;
  height: 100%;
}


.pwdIcon{
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
}