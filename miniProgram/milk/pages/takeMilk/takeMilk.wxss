@import "../../template/template.wxss";

.take_list{
  padding: 0 3%;
  margin-bottom: 20rpx;
  background-color: white;
  padding-bottom: 10rpx;
}
.take_list_head{
  height: 76rpx;
  line-height: 76rpx;
}
.list_head_L{
  float: left;
  color: #232323;
  font-size: 30rpx;
}
.list_head_R{
  float: right;
  color: #232323;
  font-size: 24rpx;
}
.list_head_R>view:nth-child(1){
  margin-right: 15rpx;
}
.list_head_R>view{
  display: inline-block;
}
.list_head_R_one>text{
  color: #e50012;
}
.list_head_R_two>text{
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-radius: 100%;
  border: 1rpx solid #232323;
  margin-right: 5rpx;
  vertical-align: middle;
}
.allSel{
  background-color: #e50012;
  border-color: #e50012 !important;
}
.take_list{
  clear: both;
}
.take_list_cont{
  margin: 10rpx 0rpx;
}
.take_list_cont>view{
  width: 74rpx;
  height: 74rpx;
  border: 1px solid #232323;
  margin-right:25rpx;
  margin-top:17rpx;
  margin-bottom: 17rpx;  
  overflow: hidden;
  border-radius: 15rpx;
  color: #232323;
  font-size: 24rpx;
  text-align: center;
  line-height: 74rpx;
  float: left;
}
.lastView{
  margin-right: 0 !important;
}
/*已取*/
.CantTake{
  background-color: #4e4e4e;
  color: white !important;
  border-color: #4e4e4e !important;
}
.take_milk_btn{
  width: 94%;
  height: 72rpx;
  background-color: #e50012;
  color: white;
  font-size: 30rpx;
  line-height: 72rpx;
  border-radius: 30rpx;
  margin-top: 90rpx;
}

.QR_layer{
  position: fixed;
  width: 477rpx;
  height: 484rpx;
  background-color: white;
  border-radius: 20rpx;
  top: 378rpx;
  margin: auto;
  left: 0;
  right: 0;
  z-index: 5;
}
.QR_code{
  display: block;
  width: 191rpx;
  height: 191rpx;
  margin: 0 auto;
  margin-top: 65rpx;
}
.QR_take_num{
  color: #000000;
  font-size: 30rpx;
  margin-top: 23rpx;
  text-align: center;
}
.QR_marked{
  width: 70%;
  margin: 0 auto;
  text-align: center;
  font-size: 24rpx;
  color: #858585;
  margin-top: 17rpx;
  line-height: 1.8;
}
.QR_close{
  position: absolute;
  top: 20rpx;
  right: 31rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  border: 1rpx solid rgba(0, 0, 0, 0.5);
  text-align: center;
  color: rgba(0, 0, 0, 0.5);
  font-size: 30rpx;
  line-height: 38rpx;
}














/*已选*/
.selectedView{
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
  border-color: #e50012 !important;
}
