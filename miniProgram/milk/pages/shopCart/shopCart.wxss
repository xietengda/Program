@import "../../template/template.wxss";

.Cart_head{
  height: 68rpx;
  line-height: 68rpx;
  background-color: white;
  padding: 0rpx 30rpx;
}
.Cart_head_L{
  float:left;
  color: #6b6b6b;
  font-size: 24rpx;
  width: 250rpx;
  line-height: 66rpx;
}
.cart_addICon{
  width: 28rpx;
  height: 38rpx;
  background-size: 100% 100%;
  float: left;
  margin-right: 20rpx;
  margin-top: 12rpx;
}
.Cart_head_R{
  float: right;
  color: #e50012;
  font-size: 24rpx;
}
.cart_cont{
  padding-bottom: 120rpx;
}
.cart_list{
  height: 220rpx;
  background-color: white;
  margin-top: 20rpx;
}
.cart_list_L{
  float: left;
}
.cart_sel{
  width: 38rpx;
  height: 38rpx;
  background-size: 100% 100%;
  float: left;
  margin-left: 30rpx;
  margin-top: 90rpx;
}
.cart_img{
  width: 164rpx;
  height: 164rpx;
  margin-left: 10rpx;
  margin-top: 25rpx;
}
.cart_list_R{
  float: left;
  height: 100%;
  width: 55%;
  margin-left: 30rpx;
  position: relative;
}
.cart_til{
  color: #353535;
  font-size: 30rpx;
  font-weight:  bold;
  margin-top: 30rpx;
}
.cart_subTil{
  font-size: 18rpx;
  color: #232323;
  margin-top: 10rpx;
}
.cart_price{
  color: #3e403f;
  font-size: 24rpx;
  position: absolute;
  bottom: 20rpx;
}
.cart_price>text{
  font-size: 46rpx;
  color: #e50012;
}
.cart_count{
  position: absolute;
  right: 0;
  bottom: 20rpx;
  color: #353535;
  background-color: #f9f9f9;
}
.cart_count>view{
  width: 40rpx;
  height: 44rpx;
  font-size:22rpx;
  float: left;
  text-align: center;
  line-height: 44rpx;
}
.cart_count>input{
  width: 50rpx;
  float: left;
  font-size: 20rpx;
  min-height: 42rpx !important;
  height: 42rpx !important; 
  text-align: center;
} 
.cart_btm{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background-color: white;
  z-index: 2;
}
.cart_btm_L{
  float: left;
  text-align: center;
  color: #353535;
  font-size: 18rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.cart_all_sel{
  width: 38rpx;
  height: 38rpx;
  margin-bottom: 5rpx;
}
.cart_btm_R{
  float: right;
}
.cart_btm_R_L{
  float: left;
  margin-right: 30rpx;
  text-align: right;
}
.cart_c_price{
  color: #353535;
  font-size: 30rpx;
  margin-top: 15rpx;
  font-weight: bold;
}
.cart_c_price>text{
  color: #e50012;
}
.cart_dis{
  font-size: 18rpx;
  color: #353535;
  margin-top: 10rpx;
}
.cart_btm_R_R{
  float: left;
  width: 230rpx;
  height: 108rpx;
  background-color: #e50012;
  color: white;
  font-size: 30rpx;
  text-align: center;
  vertical-align: middle;
}
.cart_btm_R_R>view:nth-child(1){
  margin-top: 15rpx;
}
.cart_btm_R_R>view:nth-child(2){
  font-size: 18rpx;
  margin-top: 10rpx;
}
.delCart{
  float: right;
  height: 100%;
}
.delCart>button{
  width: 195rpx;
  height: 60rpx;
  color: white;
  font-size: 24rpx;
  background-color:#e50012;
  border-radius: 50rpx;
  margin-top: 20rpx;
  margin-right: 30rpx;
}

/*空购物车*/
.nullCart{
  width: 100%;
  height: 475rpx;
  border-bottom: 1rpx solid #e5e5e5;
  text-align: center;
  padding-top: 1rpx;
}
.nullCart_icon{
  width: 230rpx;
  height: 225rpx;
  margin: 0 auto;
  margin-top: 95rpx;
}
.nullText{
  color: #6b6b6b;
  font-size: 24rpx;
  margin-top: 30rpx;
}



/*购物车*/
.bigShopCart{
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
}