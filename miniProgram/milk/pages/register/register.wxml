<import src="../../template/template.wxml" />

<view class='all_con'>
  <template is="all_head" data="{{type:1}}"/>

  <view class='reg_cont'>
      <view class='reg_cont_list'>
        <view>手机号：</view>
        <input type='text' placeholder='请填写手机号码'/>
        <button>获取验证码</button>
      </view>
      <view class='reg_cont_list'>
        <view>验证码:</view>
        <input type='text' placeholder='请输入验证码'/>
      </view>
      <view class='reg_cont_list'>
        <view>密码:</view>
        <input type='password' placeholder='请输入字母、数字或符号'/>
      </view>
      <view class='reg_cont_list'>
        <view>确认密码：</view>
        <input type='password' placeholder='请输入字母、数字或符号'/>
      </view>

      <view class='reg_regulations'>
          <checkbox  />
          <text>我已阅读并接受《香满楼服务条款》</text>
      </view>

      <button class='reg_btn'>注册</button>
  </view>

</view>
