html,
body {
	height: 100%;
}

body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
input,
textarea,
p,
blockquote,
th,
td {
	margin: 0;
	padding: 0
}

* {
	margin: 0;
	padding: 0;
}

ul li {
	list-style-type: none;
}

.divHide {
	display: none;
}

#pageDiv {
	position: fixed;
	z-index: 2;
	margin-top: -10%;
	width: 100%;
	text-align: center;
	font-size: 0.40rem;
	color: #d61013;
}

#father {
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
}

.swiper-container {
	width: 100%;
	height: 100%;
}