   /*样式*/
	/*编辑的下面四个按钮样式*/
	.footer {
		width: 100%;
		overflow: hidden;
		height: 1rem;
		position: fixed;
		left: 0;
		margin-top: 1rem;
		padding-bottom: 0.4rem;
		background-color: #fff;
		z-index: 9999;
		border-top: 1px solid #ccc;
	}
	.footer a {
		font-size: 0.25rem;
		color: #999;
		display: inline-block;
		width: 9%;
		margin: 0 11.6%;
		overflow: hidden;
		text-align: center;
		margin-top:0.1rem;
	}
	.footer a i {
		display: block;
		width: 60%;
		height: 0.5rem;
		margin: 0.1rem auto;
		margin-bottom: 0.01rem
	}
	.footer a i img {
		width: 100%
	}
	/*编辑的下面四个按钮样式*/

	



