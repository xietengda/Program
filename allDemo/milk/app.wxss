page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  font-family: '微软雅黑';
}

navigator {
  display: inline-block;
}

.all_con {
  padding-top: 125rpx;
}

.divHide {
  display: none !important;
}

.div_float:after {
  clear: both;
  content: '';
  width: 0;
  height: 0;
  visibility: hidden;
  display: block;
}

.ellipsis_one {
  overflow: hidden;
  text-overflow: ellipsis;
  display: box !important;
  display: -webkit-box !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.eli_two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: box !important;
  display: -webkit-box !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}


