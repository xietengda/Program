/*输入账号输入框和确定按钮*/

.add_adr {
  background-color: white;
  margin-top: 20rpx;
}

.add_adr>view {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #e5e5e5;
}

.add_adr>view>view:nth-child(1) {
  float: left;
  line-height: 90rpx;
  color: 28rpx;
  font-size: 30rpx;
  width: 170rpx;
}

.add_adr>view>input {
  float: left;
  max-width: 60%;
  line-height: 90rpx;
  height: 100%;
  font-size: 28rpx;
}

.add_adr_btn {
  width: 90%;
  height: 70rpx;
  background-color: #e50012;
  border-radius: 25rpx;
  color: white;
  font-size: 40rpx;
  margin-top: 30rpx;
  line-height: 70rpx;
}

.phcolor {
  color: #bbb;
}

