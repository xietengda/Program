@import "../../template/template.wxss";

.ad_y>image {
  width: 750rpx;
  height: 240rpx;
}

.score_img {
  width: 100%;
  height: 275rpx;
  display: block;
}

.score_head_menu {
  width: 100%;
  height: 190rpx;
  margin-top: 10rpx;
  background-color: white;
}

.score_head_menu>navigator {
  display: block;
}

.score_menu_list {
  float: left;
  width: 25%;
  height: 100%;
  text-align: center;
  font-size: 26rpx;
}

.score_menu_list_icon {
  width: 65rpx;
  height: 65rpx;
  margin: 0 auto;
  margin-top: 37rpx;
  background-size: 100% 100% !important;
}

.icon_s {
  width: 55rpx;
}

.score_menu_list>view {
  margin-top: 23rpx;
}

.score_cont {
  margin-top: 5rpx;
  background-color: white;
  padding-top: 30rpx;
}

.score_cont_til {
  width: 95%;
  margin: 0 auto;
}

.score_cont_img {
  margin-top: 20rpx;
}

.score_cont_list {
  width: 95%;
  margin: 0 auto;
  display: flex;
  justify-content: flex-start;
  flex-flow: wrap;
  margin-top: 50rpx;
}

.score_pro {
  width: 240rpx;
  height: 450rpx;
  margin-bottom: 20rpx;
  margin-left: 60rpx;
}

.score_pro_right {
  margin-right: 80rpx;
}

.score_pro_img {
  width: 220rpx;
  height: 220rpx;
  margin: 0 auto;
  margin-top: 15rpx;
}

.score_pro_til {
  color: #333;
  font-size: 28rpx;
  margin-top: 10rpx;
}

.score_pro_price {
  color: #f52039;
  font-size: 26rpx;
  margin-top: 10rpx;
}

.score_pro_oldPri {
  color: #606060;
  font-size: 20rpx;
  margin-top: 10rpx;
}

.score_pro_buy::after {
  border: none;
}

.score_pro_buy {
  width: 173rpx;
  height: 48rpx;
  border-radius: 20rpx;
  background-color: #e50012;
  color: white;
  font-size: 24rpx;
  display: inline-block;
  line-height: 48rpx;
  margin-top: 20rpx;
}

.makeMoney {
  background: url('data:image/png;base64,') center center no-repeat;
}

.checkScore {
  background: url('data:image/png;base64,') center center no-repeat;
}

.classify {
  background: url('data:image/png;base64,') center center no-repeat;
}

.orderList {
  background: url('data:image/png;base64,') center center no-repeat;
}
