  background-color: #e79e12;
  line-height: 55rpx;
  vertical-align: middle;
  font-size: 24rpx;
  margin-top: 20rpx;
  padding: none !important;
}

.my_head_combo_R>navigator>button::after {
  border: none;
}

.my_head_combo_R>navigator:nth-child(2)>button {
  background-color: #e50012;
