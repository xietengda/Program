@import "../../template/template.wxss";

image {
  display: inline;
}

.my_head {
  width: 100%;
  height: 175rpx;
  background-color: #e50012;
  font-size: 0;
}

.my_head .userAvatarUrl {
  float: left;
  overflow: hidden;
  width: 130rpx;
  height: 130rpx;
  margin: 20rpx;
  border-radius: 50%;
  /* border: 2rpx solid #fff; */
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.my_head>image {
  width: 135rpx;
  height: 135rpx;
  border-radius: 100%;
  float: left;
  margin-left: 35rpx;
  margin-top: 25rpx;
}

